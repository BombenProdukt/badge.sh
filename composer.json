{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "framework",
        "laravel"
    ],
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-heroicons": "^2.1",
        "composer/semver": "^3.3",
        "graham-campbell/bitbucket": "^10.0",
        "graham-campbell/github": "^12.2",
        "graham-campbell/gitlab": "^7.0",
        "guzzlehttp/guzzle": "^7.2",
        "iconify/json": "^2.2",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^3.0",
        "laravel/octane": "^1.5",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^2.11",
        "moneyphp/money": "^4.1",
        "preemstudio/formatter": "^1.0",
        "preemstudio/laravel-badger": "^1.0",
        "spatie/laravel-data": "^3.2",
        "spatie/laravel-responsecache": "^7.4",
        "spatie/regex": "^3.1",
        "spiral/roadrunner": "^2.8.2",
        "symfony/dom-crawler": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.30",
        "fakerphp/faker": "^1.9.1",
        "graham-campbell/analyzer": "^4.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.4",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.2",
        "spatie/laravel-ignition": "^2.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
